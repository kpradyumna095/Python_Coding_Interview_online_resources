      "source": [
        "from datetime import date\n",
        "\n",
        "def calculateAge(birthDate):\n",
        "  today = date.today()\n",
        "  age = today.year - birthDate.year -((today.month, today.day) < (birthDate.month, birthDate.day))\n",
        "  return age\n",
        "\n",
        "print(calculateAge(date(1997, 2, 3)), \"years\")\n",
        "\n"
      ],
